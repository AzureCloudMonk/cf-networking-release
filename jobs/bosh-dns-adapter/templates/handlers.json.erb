<% if p('cf_app_sd_disable') %>
<%=

  config = []

require 'json'
JSON.dump(config)
%>
<% else %>
 <%=

config = p('internal_domains').map{
  |domain| {
    'domain' => domain,
    'cache' => {'enabled' => false},
    'source' => {
      'type' => 'http',
      'url' => "http://#{p('address')}:#{p('port')}"
    }
  }
}

require 'json'
JSON.dump(config)
%>

<% end %>
